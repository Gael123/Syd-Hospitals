<p id="notice"><%= notice %></p>

<h1>Diseases</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Disease Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diseases.each do |disease| %>
      <tr>
        <td><%= disease.name %></td>
        <td><%= disease.disease_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
